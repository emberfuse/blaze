{
    "name": "emberfuse/blaze",
    "type": "library",
    "description": "Tailwind and Inertia scaffolding for Laravel.",
    "keywords": [
        "emberfuse",
        "tailwind",
        "auth"
    ],
    "license": "MIT",
    "support": {
        "issues": "https://github.com/emberfuse/blaze/issues",
        "source": "https://github.com/emberfuse/blaze"
    },
    "authors": [
        {
            "name": "Thavarshan Thayananthajothy",
            "email": "tjthavarshan@gmail.com"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "ext-json": "*",
        "ext-gmp": "*",
        "ext-bcmath": "*",
        "emberfuse/scorch": "^1.1",
        "hashids/hashids": "^4.1",
        "illuminate/support": "^8.0"
    },
    "require-dev": {
        "guzzlehttp/guzzle": "^7.2",
        "inertiajs/inertia-laravel": "^0.4.0",
        "mockery/mockery": "^1.4.2",
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "Emberfuse\\Blaze\\": "src/"
        },
        "files": [
            "src/Testing/Support/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Emberfuse\\Blaze\\Tests\\": "tests/",
            "App\\": "stubs/app/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-main": "1.x-dev"
        },
        "laravel": {
            "providers": [
                "Emberfuse\\Blaze\\Providers\\BlazeServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
